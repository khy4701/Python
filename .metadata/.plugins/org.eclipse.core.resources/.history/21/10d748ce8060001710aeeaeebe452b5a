# -*- coding:utf-8 -*-
import os
from bs4 import BeautifulSoup


f = open("D:/Users/Ariel/workspace/PythonFileReading/parsing.txt", 'r')

initXml =''
endXml =''

initFlag = 1

count =0
while True:
    line = f.readline()

    if '<tr height="25">' in line:
        initFlag = 0

    if initFlag == 0 and '<tr height="30">' in line:
        endFlag = 1
    
    if not line:
        break

    if initFlag == 1:
        initXml += line
        continue
    
    if endFlag == 1:
        endXml += line
        continue
    
f.close()




print( initXml)

