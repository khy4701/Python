import os
from bs4 import BeautifulSoup


class parsingXML():
    
    initXml = ''
    def __init(self):
        
        
    def getXml(self):
        return self.initXml

f = open("D:/Users/Ariel/workspace/PythonFileReading/parsing.txt", 'r')

while True:
    line = f.readline()

self.initXml += line

if not line:
    break

f.close()


xml = parsingXML()
print(xml.getXml())


